<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.clapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/clapp_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/clappround"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Library"
            android:label="@string/activity_library">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OnPlay"
            android:label="@string/activity_on_play"/>
        <activity android:name=".Metadata"
            android:label="@string/activity_metadata">
        </activity>
    </application>

</manifest>